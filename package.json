{
  "name": "complyze",
  "version": "0.1.0",
  "description": "Complyze â€” AI Tool Intelligence Extraction & Risk Assessment Pipeline",
  "workspaces": [
    "web",
    "desktop"
  ],
  "scripts": {
    "build:web": "npm run build -w web",
    "dev": "tsx watch src/index.ts",
    "dev:web": "npm run dev -w web",
    "dev:app": "npm run dev -w desktop",
    "launch": "concurrently --kill-others -n \"WEB,APP\" \"npm run dev:web\" \"npm run dev:app\"",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "diagnose": "npm run diagnose -w web",
    "proxy": "npm run proxy -w web"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^3.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}