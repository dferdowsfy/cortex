{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/web/$1"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/web/$1"
        }
    ]
}